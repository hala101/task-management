                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": { "description": "(Required) ", "example": "Design Landing Page", "type": "string" },
                                    "priority": { "description": "(Required) ", "example": "high", "type": "string" },
                                    "dueDate": { "description": "(Required) ", "example": "2024-07-28", "type": "string" },
                                    "category": { "description": "(Required) ", "example": "UI/Ux", "type": "string" },
                                    "status": { "description": "(Required) status", "example": "in progress", "type": "string" },
                                    "notes": { "description": "(Required) ", "example": "Please complete ASAP", "type": "string" },
                                    "attachments": { "description": "(Required) ", "type": "string", "format": "binary" },
                                    "isTemplate": { "example": "true", "type": "string" },
                                    "recurrence": { "description": "daily | weekly | monthly | yearly | null", "example": "weekly", "type": "string" }
                                },
                                "required": ["name", "priority", "dueDate", "category", "status", "notes", "attachments", "isTemplate", "recurrence"]
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "error": { "type": "string" },
                                        "message": { "type": "string" },
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "userId": { "type": "string" },
                                                "name": { "type": "string" },
                                                "priority": { "type": "string" },
                                                "dueDate": { "type": "string" },
                                                "category": { "type": "string" },
                                                "status": { "type": "string" },
                                                "notes": { "type": "string" },
                                                "attachments": { "type": "array", "items": { "type": "string" } },
                                                "collaborators": { "type": "array", "items": { "type": "string" } },
                                                "_id": { "type": "string" },
                                                "__v": { "type": "string" }
                                            },
                                            "required": [
                                                "userId",
                                                "name",
                                                "priority",
                                                "dueDate",
                                                "category",
                                                "status",
                                                "notes",
                                                "attachments",
                                                "collaborators",
                                                "_id",
                                                "__v"
                                            ]
                                        }
                                    },
                                    "required": ["error", "message", "data"]
                                },
                                "examples": {
                                    "1": {
                                        "summary": "Success",
                                        "value": {
                                            "error": "<boolean>",
                                            "message": "<string>",
                                            "data": {
                                                "userId": "<string>",
                                                "name": "<string>",
                                                "priority": "<string>",
                                                "dueDate": "<string>",
                                                "category": "<string>",
                                                "status": "<string>",
                                                "notes": "<string>",
                                                "attachments": ["<string>", "<string>"],
                                                "collaborators": ["<string>", "<string>"],
                                                "_id": "<string>",
                                                "__v": "<integer>"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [{ "bearer": [] }]
            }
